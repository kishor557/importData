%p#notice= notice
%h1 Import Sheets
%table
  %thead
    %tr
      %th File Name
      %th File Size
      %th Import Date
      %th{:colspan => "3"}
  %tbody
    - @sheets.each do |sheet|
      %tr
        %td= sheet.sheet_file_name
        %td= sheet.sheet_file_size
        %td= sheet.created_at
        %td= link_to 'Process', process_sheet_import_sheet_path(sheet.id)
        
= link_to 'New Sheet', new_import_sheet_path  
%br
= link_to 'View All Participants', participants_path
%br
= link_to 'View All Events', events_path
